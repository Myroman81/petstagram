{{log this}}
{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <!-- Call helper function with triple brackets to render the emoji span as HTML -->
      <a href="/posts/{{post.id}}">{{post.title}}</a>
    </h2>
    <p>
      {{post.body}}
    </p>
    <p>Created by <a href="/users/{{post.user_id}}">{{post.user.name}}</a></p>
  </div>
  <div class="col-md-7">
  </div>
</div>
{{/each}}
